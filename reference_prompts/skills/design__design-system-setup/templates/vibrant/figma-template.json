  "figmaConfig": {
    "colorStyles": {
      "createFromTokens": true,
      "naming": "{colorFamily}/{shade}",
      "includeGradients": true
    },
    "textStyles": {
      "createFromTokens": true,
      "naming": "{family}/{size}/{weight}",
      "displayFont": "Poppins",
      "bodyFont": "Work Sans",
      "accentFont": "Fredoka One"
    },
    "effectStyles": {
      "createFromTokens": true,
      "naming": "Shadow/{size}",
      "includeDramaticShadows": true
    },
    "gradients": [
      {
        "name": "Pink to Purple",
        "colors": ["#FF1A94", "#941AFF"],
        "angle": 135
      },
      {
        "name": "Orange to Pink",
        "colors": ["#FF881A", "#FF1A94"],
        "angle": 90
      },
      {
        "name": "Teal to Purple",
        "colors": ["#1AC7C7", "#941AFF"],
        "angle": 135
      }
    ]
  },
  "designPhilosophy": {
    "colors": "Multi-color palette encourages creative mixing",
    "typography": "Bold display fonts create impact and personality",
    "spacing": "Generous spacing creates breathing room",
    "shadows": "Dramatic shadows add depth and playfulness",
    "borders": "Rounded corners soften the interface"
  }
